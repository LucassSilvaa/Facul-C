        scanf ("%f", &notas[i]);
    }
    for (i=0; i <=9; i++){
        if (i==7){
            printf("A posição [%d] do vetor contém a nota : %.1f \n", i, notas[i] );
    }else if (i==9){
        printf("A posição [%d] do vetor contém a nota : %.1f \n", i, notas [i]);
    }
    }
